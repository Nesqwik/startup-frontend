{
  "name": "frontend",
  "version": "0.1.0-SNAPSHOT",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.5",
    "flow-bin": "^0.56.0",
    "material-ui": "^0.19.4",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-scripts": "1.0.14",
